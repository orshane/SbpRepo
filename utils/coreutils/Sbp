PORTNAME=coreutils
VERSION=9.4
FETCH_CMD=curl -L https://ftp.gnu.org/gnu/coreutils/coreutils-9.4.tar.xz -o /usr/local/var/sbpi/distfiles/coreutils-9.4.tar.xz
BUILD_CMD=tar -xf /usr/local/var/sbpi/distfiles/coreutils-9.4.tar.xz -C /usr/local/var/sbpi/work && cd /usr/local/var/sbpi/work/coreutils-9.4 && ./configure --prefix=/usr/local && make V=1
INSTALL_CMD=cd /usr/local/var/sbpi/work/coreutils-9.4 && sudo make install
UNINSTALL_CMD=cd /usr/local/var/sbpi/work/coreutils-9.4 && sudo make uninstall
